    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "@mediumGray@"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "@softGreen@",
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "@softRed@"
            }
        },
        {
            "scope": [
                "entity.name.class",
                "entity.name.struct",
                "entity.name.enum",
                "entity.name.union",
                "entity.name.trait",
                "entity.name.interface",
                "entity.name.impl",
                "entity.name.type -variable.other.enumMember",
                "entity.other.inherited-class",
                "support.type",
                "storage.type -storage.type.js"
            ],
            "settings": {
                "foreground": "@softViolet@",
            }
        },
        {
            "scope": [
                "entity.name.namespace",
                "entity.other.attribute-name.namespace"
            ],
            "settings": {
                "foreground": "@softYellow@"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.modifier",
                "constant.language",
                "storage.type.js",
                "storage.type.function.js"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.other -variable.other.property -entity.name",
                "variable.language",
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "@vividRed@",
                "fontStyle": "bold underline",
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "@vividYellow@",
                "fontStyle": "bold underline"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded"
            ],
            "settings": {
                "foreground": "@fg@"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "@bgBlue@"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "@vividViolet@"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold underline"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "@bgGreen@"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "@bgRed@"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "@bgBlue@"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.raw",
            "settings": {
                "foreground": "@softGreen@"
            }
        },
        {
            "scope": "entity.name.section",
            "settings": {
                "foreground": "@emphFg@",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "@softViolet@",
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "@softMagenta@"
            }
        },
        {
            "scope": "punctuation.separator.namespace",
            "settings": {
                "foreground": "@fg@"
            }
        },
        {
            "scope": [
                "meta.preprocessor",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "@softBlue@"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "@softGreen@"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "@vividGreen@"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "@vividYellow@"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "@vividRed@"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "@vividMagenta@"
            }
        },
        {
            "scope": "string support.type.property-name",
            "settings": {
                "foreground": "@softViolet@"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*.mutable:rust": {
            "fontStyle": ""
        },
        "variable.mutable:rust": {
            "fontStyle": "italic"
        },
        "parameter.mutable:rust": {
            "fontStyle": "italic"
        },
        "variable.static:rust": {
            "fontStyle": ""
        },
        "selfKeyword:rust": {
            "fontStyle": "bold italic"
        },
        "*.unsafe:rust": {
            "foreground": "@softRed@"
        }
    }
}
